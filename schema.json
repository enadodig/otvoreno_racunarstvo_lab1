    "properties": {
      "genre_id": {
        "type": "number"
      },
      "name": {
        "type": "string"
      },
      "era_of_origin": {
        "type": "string"
      },
      "years_active": {
        "type": "string"
      },
      "artist": {
        "type": "string"
      },
      "top_ranked_album": {
        "type": "string"
      },
      "origin_country": {
        "type": "string"
      },
      "main_instruments": {
        "type": "string"
      },
      "typical_bpm_range": {
